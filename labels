traefik.enable=true
traefik.http.services.traefik-sceneapi.loadbalancer.server.port=8080
traefik.http.routers.sceneapi.rule=Host(`scene-api.dapp-craft.com`)
traefik.http.routers.sceneapi.entrypoints=http,https
traefik.http.routers.sceneapi.tls=true
traefik.http.routers.sceneapi.tls.certresolver=letsEncrypt
traefik.http.middlewares.sceneapi.headers.accesscontrolallowmethods=GET,OPTIONS,PUT
traefik.http.middlewares.sceneapi.headers.accesscontrolalloworiginlist=*
traefik.http.middlewares.sceneapi.headers.accesscontrolmaxage=100
traefik.http.middlewares.sceneapi.headers.addvaryheader=true
